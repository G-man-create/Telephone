<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<DialogPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <GridPane hgap="10" vgap="10">
            <Label text="Номер:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="numberField" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Тип:" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <ComboBox fx:id="typeComboBox" GridPane.rowIndex="1" GridPane.columnIndex="1">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="Мобильный"/>
                        <String fx:value="Домашний"/>
                        <String fx:value="Рабочий"/>
                        <String fx:value="Другой"/>
                    </FXCollections>
                </items>
            </ComboBox>
        </GridPane>
    </content>
</DialogPane>